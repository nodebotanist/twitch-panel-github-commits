<html lang="">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="x-ua-compatible" content="ie=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link href="https://fonts.googleapis.com/css?family=Source+Code+Pro|VT323" rel="stylesheet">
        <link rel="stylesheet" href="https://unpkg.com/purecss@1.0.0/build/pure-min.css" integrity="sha384-nn4HPE8lTHyVtfCBi5yW9d20FjT8BJwUXyWZT9InLYax14RDjBj46LmSztkmNP9w" crossorigin="anonymous">
        <link rel="stylesheet" href="css/common.css">
        <link rel="stylesheet" href="css/viewer.css">
    </head>
    <body>
        <div class="pure-g">
            <div class="title pure-u-1">
                <h1>Git Commits for this Streamer!</h1>
            </div>
            <ul class="js-commit-list">
                <% console.log(commits); commits.forEach((commit) => { %>
                    <li>
                        <a class="commit-repo" href="<%= commit.repository.html_url %>" target="_blank" >
                            <%= commit.repository.name %>
                        </a>: 
                        <a class="commit-message" href="<%= commit.html_url %>" target="_blank">
                            <%= commit.commit.message %>
                        </a>
                        </li> 
                <% }) %>
            </ul>
        </div>
        <script src="https://extension-files.twitch.tv/helper/v1/twitch-ext.min.js"></script>
        <script src="js/common.js"></script>
        <script src="js/viewer.js"></script>
    </body>
</html>